<?xml version="1.0" encoding="UTF-8"?>


<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  	
  
  <mapper namespace="com.h1b4.www.teacher.dao.TeacherMapper">
  	
  	<!--회원정보 등록  -->
	<insert id="selectTeacherList" parameterType="teacher" >
	insert into Teacher(
		teacher_id
		,teacher_password
		,teacher_name
		,teacher_ip
		
	)values(
		#{teacher_id}
		,#{teacher_password}
		,#{teacher_name}
		,#{teacher_ip}
		
	)
  </insert>
  

  	
  </mapper>