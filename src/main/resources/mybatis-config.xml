<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	
 	<typeAliases>
		<typeAlias type="com.h1b4.www.vo.Transcript" alias="transcript" />
		<typeAlias type="com.h1b4.www.vo.Contents" alias="contents"/>
		<typeAlias type="com.h1b4.www.vo.Member" alias="member"/>
		<typeAlias type="com.h1b4.www.vo.Teacher" alias="teacher"/>
		<typeAlias type="com.h1b4.www.vo.Emotion" alias="emotion"/>
		<typeAlias type="com.h1b4.www.vo.ContentsReply" alias="contentsreply"/>
		<typeAlias type="com.h1b4.www.vo.Bookmark" alias="bookmark"/>
		<typeAlias type="com.h1b4.www.vo.Category" alias="category"/>
		<typeAlias type="com.h1b4.www.vo.Voca" alias="voca"/>
	</typeAliases>
	
	<mappers>
		<mapper resource="contentsMapper/ContentsMapper.xml"/>
		<mapper resource="contentsMapper/ContentsReplyMapper.xml"/>
		<mapper resource="contentsMapper/ContentsRecommendMapper.xml"/>
		<mapper resource="memberMapper/MemberMapper.xml"/>
		<mapper resource="memberMapper/TeacherMapper.xml"/>
		<mapper resource="transcriptMapper/ContentsMapper.xml" />
		<mapper resource="transcriptMapper/TranscriptMapper.xml" />
		<mapper resource="transcriptMapper/VocaMapper.xml" />
	</mappers>
</configuration>
